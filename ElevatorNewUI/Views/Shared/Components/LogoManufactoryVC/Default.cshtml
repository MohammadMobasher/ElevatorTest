@using DataLayer.Entities
@model List<LogoManufactory>

@using DNTPersianUtils.Core




<div class="adplacement-container-row">
    @if (Model != null)
    {
        
        foreach (var item in Model)
        {
            <div class="col-6 col-lg-3 pr">
                <a href="@item.URL" class="adplacement-item mb-4">
                    <div class="adplacement-sponsored_box">
                        <img asp-append-version="true" src="@("http://staffonly.ir/" + "" + item.AddressImg)" alt="@(item.Alt + ", آسانسور")">
                    </div>
                </a>
            </div>
        }
    }


</div>